<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 默认时的背景图片 -->
    <item android:drawable="@drawable/start"/>

    <!-- 选中时的图片背景 -->
    <item android:drawable="@drawable/start_pressed" android:state_selected="true"/>

    <!-- 获得焦点时的图片背景 -->
    <item android:drawable="@drawable/start_pressed" android:state_focused="true"/>

</selector>